<MyPopup1@Popup>
    id: MyPopup1
    auto_dissmis: False
    title: ' '
    size_hint: 0.9,0.55
    pos_hint: {'center_x':0.5, 'center_y':0.7}
    separator_color: 236/255, 236/255, 236/255, 1
    background: 'picture\EC.png'
    FloatLayout:
        id: float
        size_hint: 1,1
        MDIconButton:
            icon: 'close-outline'
            pos_hint: {'center_x':0.97,'center_y':1.085}
            user_font_size: "35sp"
            on_release: root.dismiss()
        Image: 
            source: 'PopupImages/baemin.png'
            pos_hint: {'center_x':0.5, 'center_y': 0.9}
            allow_stretch: True
            size_hint_x: 0.45
        
        Button:
            text: '취소'  #cancel
            font_name: 'font\IBMPlexSansKR-Light.ttf'
            color: 0,0,0,1
            background_color: 1,1,1,1
            background_normal: ''
            size_hint: (0.2,0.1)
            pos_hint: {'center_x': 0.37, 'center_y': 0.25}
            on_release: 
                root.dismiss()
       
        Label:
            markup: True
            text: '입력하신 정보는'
            font_name: 'font\IBMPlexSansKR-ExtraLight.ttf'
            font_size: 17
            color: 0,0,0,1
            pos_hint: {'center_x': 0.5, 'center_y': 0.12}
        Label:
            markup: True
            text: '암호화되어 보관됩니다.'
            font_name: 'font\IBMPlexSansKR-ExtraLight.ttf'
            font_size: 17
            color: 0,0,0,1
            pos_hint: {'center_x': 0.5, 'center_y': 0.05}

