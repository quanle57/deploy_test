<ShopLayout>: 
    FloatLayout:
        id: floatshop
        # size: root.width, 300
        width: root.width
        height: 300

        size_hint_x: None
        size_hint_y: None
        spacing:2
        Label:
            markup: True
            text: '지점'
            font_name: 'font\IBMPlexSansKR-SemiBold.ttf'
            font_size: 18
            color: 51/255, 119/255, 106/255, 1
            pos_hint: {'center_x': 0.08, 'center_y': 0.92}
        MDTextFieldRect:
            id: shop
            hint_text: "스쿨푸드 신림 "
            font_name: 'font\IBMPlexSansKR-Light.ttf'
            color: 0,0,0,1
            size_hint: 0.4, None
            height: '35dp'
            pos_hint: {'center_x': 0.35, 'center_y': 0.92}
        Button:
            id: update
            text: '수정'
            font_name: 'font\IBMPlexSansKR-Bold.ttf'
            color: 1,1,1,1
            background_color: 51/255, 119/255, 106/255, 1
            background_normal: ''
            size_hint: 0.15, None
            height: 35
            pos_hint: {'center_x': 0.65, 'center_y': 0.92}
            on_press: root.updatedata()
        Button:
            id: delete
            text: '지점삭제'
            font_name: 'font\IBMPlexSansKR-Bold.ttf'
            color: 1,1,1,1
            background_color: 51/255, 119/255, 106/255, 1
            background_normal: ''
            size_hint: 0.22, None
            height: 35
            pos_hint: {'center_x': 0.85, 'center_y': 0.92}
            on_press: root.deletedata()
        BoxLayout:
            size_hint: 1, None
            orientation: 'vertical'
            spacing:5
            pos_hint: {'center_x':0.48,'center_y':0.53}
            Button:
                id: baemin
                size_hint: None, None
                width: 210
                height:40
                background_normal: 'picture/baemin.png'
                pos_hint: {'center_x':0.51}
                on_release: root.load_site_baemin()
            Button:
                id: coupang
                size_hint: None, None
                width: 210
                height:40
                background_normal: 'picture/coupang_eats.png'
                pos_hint: {'center_x':0.51}
                on_release: root.load_site_coupang()
            Button:
                id: yogi
                size_hint: None, None
                width: 210
                height:40
                background_normal:  'picture\yogi.png'
                pos_hint: {'center_x':0.51}
                on_release: root.load_site_yogiyo()
        Button:
            text: '최초 정보 수집'
            font_name: 'font\IBMPlexSansKR-Bold.ttf'
            color: 1,1,1,1
            background_color: 51/255, 119/255, 106/255, 1
            background_normal: ''
            size_hint: None,None
            width: 140
            height: 40
            pos_hint: {'center_x': 0.48, 'center_y': 0.25}
            
        Label: 
            markup: True
            text: '최초 입력 시 지난 1개월의 정보를 수'
            font_name: 'font\IBMPlexSansKR-Light.ttf'
            font_size: 15
            color: 0,0,0, 1
            pos_hint: {'center_x': 0.48, 'center_y': 0.14}
        Label: 
            markup: True
            text: '집합니다. (30분 예상)'
            font_name: 'font\IBMPlexSansKR-Light.ttf'
            font_size: 15
            color: 0,0,0, 1
            pos_hint: {'center_x': 0.48, 'center_y': 0.08}
    